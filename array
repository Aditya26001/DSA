#include<iostream>

using namespace std;
int main(int argc, char const *argv[])
{
    int arr1[4] = {1,3,4,5};
    int arr2[4] = {2,4,6,8};
    int arr3[8] ;


    int i = 0, j = 0;
    int k = 0;

    while(i<4&&j<4){
        if(arr1[i]<arr2[j])
            arr3[k++] = arr1[i++];

        else
            arr3[k++] = arr2[j++];
    }    

    while (i<4)
    {
        arr3[k++] = arr1[i++];
    }
    
    while (j<4)
    {
        arr3[k++] = arr2[j++];
    }
    
    for(int m =0 ; m<8 ; m++){
        cout<<arr3[m];
        cout<<" ";

    }
    return 0;
}



 
